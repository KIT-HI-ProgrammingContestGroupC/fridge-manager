generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model fridge_items{
  id              Int @id @default(autoincrement())
  owner_name      String
  uploaded_at     DateTime @default(now())
  product_name    String
  eating_allowed  Boolean @default(false)
  image_url       String
}